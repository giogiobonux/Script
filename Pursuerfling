uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift then
		sprinting1 = true
		ws = 24
		if ability == false then
			hum.WalkSpeed = 24
		end
	end
end)
uis.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift then
		sprinting1 = false
		ws = 12
		if ability == false then
			hum.WalkSpeed = 12

		end
	end
end)

local function onButton1Down()
	if id ~= Swing and ability == false and mouse1 == false and stalking == false then 
		mouse1 = true
		local Anim2
		local animation1 = game:GetObjects("rbxassetid://" .. Swing)[1]
		animation1.Parent = char
		Anim2 = AnimHandler.new(char, animation1)
		Anim2:Play()
		Anim:Stop()
		id = Swing
		wait(.3)
		attackvel.Enabled = true
		wait(0.1)
		attackvel.Enabled = false
		wait(.2)
		Anim2:Destroy()
		animation1:Destroy()
		mouse1 = false
	end
end

mouse.Button1Down:Connect(onButton1Down)

uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.E and ability == false and stalking == false then
		ability = true
		hum.WalkSpeed = 0.1
		Anim:Stop()
		animation = game:GetObjects("rbxassetid://" .. Howl)[1]
		Anim = AnimHandler.new(char, animation)
		Anim:Play()
		wait(1.9)
		hum.WalkSpeed = ws
		ability = false
		id = Howl
	end
end)

uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.Q and ability == false and Cleaving == false and stalking == false then
		Cleaving = true
		local Anim2
		local animation1 = game:GetObjects("rbxassetid://" .. Cleave)[1]
		animation1.Parent = char
		Anim2 = AnimHandler.new(char, animation1)
		Anim2:Play()
		Anim:Stop()
		id = Cleave
		wait(.5)
		attackvel.Enabled = true
		wait(0.1)
		attackvel.Enabled = false
		wait(.6)
		Anim2:Destroy()
		animation1:Destroy()
		Cleaving = false
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.R and ability == false and stalking == false then
		ability = true
		hum.WalkSpeed = 0.1
		Anim:Stop()
		animation = game:GetObjects("rbxassetid://" .. StalkStart)[1]
		Anim = AnimHandler.new(char, animation)
		Anim:Play()
		wait(2.3)
		ability = false
		stalking = true
		id = StalkStart
		hum.WalkSpeed = ws
	elseif  input.KeyCode == Enum.KeyCode.R and ability == false and stalking == true then
		stalking = false
		ability = true
		hum.WalkSpeed = 0.1
		Anim:Stop()
		animation = game:GetObjects("rbxassetid://" .. StalkEnd)[1]
		Anim = AnimHandler.new(char, animation)
		Anim:Play()
		wait(2)
		ability = false
		stalking = false
		id = StalkEnd
		hum.WalkSpeed = ws
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.G then
		if EmoteGui.Enabled == false then
			EmoteGui.Enabled = true
		elseif EmoteGui.Enabled == true then
			EmoteGui.Enabled = false
		end
	end
end)

